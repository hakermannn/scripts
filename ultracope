pcall(function()
if newhealth < old then
time = 10
harddamage = math.clamp((owner.Character.Humanoid.MaxHealth - newhealth) * 0.8,0,99)
old = newhealth
elseif newhealth > old then
old = newhealth
end
end)
end)
