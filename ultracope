local min = -15
local max = 15
function splat(whatpart,times)
for i = 1,(2 * times) do
wait()
coroutine.wrap(function()
local b = p:Clone()
b.Parent = script
b.Position = whatpart.Position
b.Velocity = Vector3.new(Random.new():NextNumber(min,max),Random.new():NextNumber(min,max) + 30,Random.new():NextNumber(min,max))
table.insert(rayblacklist,b)
b.Touched:Connect(function(what)
if not (what:FindFirstAncestorWhichIsA("Model") and what:FindFirstAncestorWhichIsA("Model"):FindFirstChildWhichIsA("Humanoid")) and not table.find(rayblacklist,what) and what.CanCollide then
	local params = RaycastParams.new()
	params.FilterDescendantsInstances = {rayblacklist,whatpart:FindFirstAncestorWhichIsA("Model"):GetDescendants()}
	params.FilterType = Enum.RaycastFilterType.Blacklist
	local result = workspace:Raycast(b.Position,b.Velocity + Vector3.new(0,-3,0), params)
if result then
    local bloodpartfloor = Instance.new("SpawnLocation", workspace)
bloodpartfloor.Enabled = false
bloodpartfloor.CanCollide = false
bloodpartfloor.Anchored = true
table.insert(rayblacklist,bloodpartfloor)
                            bloodpartfloor.Size = Vector3.new(0.05,0.005,0.005)
                            bloodpartfloor.CanCollide = false
                            bloodpartfloor.Material = "Glass"
                            bloodpartfloor.Transparency = 0
local s = Instance.new("SpecialMesh",bloodpartfloor)
s.MeshType = "Sphere"
b:Destroy()
bloodpartfloor.Position = result.Position
local whathit = result.Instance
		local currentRightVector = whathit.CFrame.RightVector
		local upVector = result.Normal
		local newFacialVector = currentRightVector:Cross(upVector)
                            bloodpartfloor.CFrame = CFrame.fromMatrix(bloodpartfloor.Position, currentRightVector, upVector, newFacialVector) * CFrame.Angles(0,0,math.rad(90))
                                bloodpartfloor.BrickColor = BrickColor.new("Maroon")
game:GetService("Debris"):AddItem(bloodpartfloor,25)
local random = Random.new():NextNumber(0.25,3)
local tweenService = game:GetService("TweenService")
local timeToFade = random/3
local object = bloodpartfloor
local tweenInfo = TweenInfo.new(timeToFade)
local goal = {}
goal.Size = Vector3.new(math.clamp(random,0.75,3)/5,random,random)
goal.Transparency = 0.2
local groww = tweenService:Create(object, tweenInfo, goal)
groww:Play()
end
end
end)
end)()
end
end
local punch = Instance.new("Sound")
punch.PlayOnRemove = true
local punchtab = {"3932504231","4306980885","3932506625","3932506183"}
punch.Volume = 1
local candamage = false
local punc = false
e.OnServerEvent:Connect(function(_,cf)
if not punc then
punc = true
punchwel.Enabled = true
m2.MeshId = "rbxassetid://9924080097"
m2.TextureId = "rbxassetid://8550010558"
m2.Scale = Vector3.new(0.055,0.055,0.055)
weld4.Enabled = false
local tweenservice = game:GetService("TweenService")
local g = {}
local l = owner.Character.Torso.CFrame:toObjectSpace(cf).lookVector.unit
g.C0 = CFrame.new(-0.125,0.75,0.25) * CFrame.Angles(math.rad(90) + l.y,math.rad(10),0)
local i = TweenInfo.new(0.125, Enum.EasingStyle.Quint,Enum.EasingDirection.Out)
t = tweenservice:Create(punchwel, i, g) 
t:Play()
t.Completed:Wait()
candamage = true
local g = {}
g.C0 = CFrame.new(-0.125,1.5,0.0625) * CFrame.Angles(math.rad(70) + l.y,math.rad(12),math.rad(20))
local i = TweenInfo.new(0.125, Enum.EasingStyle.Quint,Enum.EasingDirection.Out)
t = tweenservice:Create(punchwel, i, g) 
t:Play()
t.Completed:Wait()
local g = {}
g.C0 = CFrame.new(-0.125,0.5,0.125) * CFrame.Angles(math.rad(100) + l.y,math.rad(10),0)
local i = TweenInfo.new(0.25, Enum.EasingStyle.Circular,Enum.EasingDirection.Out)
t = tweenservice:Create(punchwel, i, g) 
t:Play()
t.Completed:Wait()
wait(0.0625)
candamage = false
local g = {}
g.C0 = CFrame.new(0,1.5,0)
local i = TweenInfo.new(0.125, Enum.EasingStyle.Quart,Enum.EasingDirection.Out)
t = tweenservice:Create(punchwel, i, g) 
t:Play()
t.Completed:Wait()
punchwel.Enabled = false
weld4.Enabled = true
punc = false
m2.MeshId = "rbxassetid://8549147351"
m2.Scale = Vector3.new(0.025,0.025,0.025)
end
end)
owner.Character.Humanoid.Touched:Connect(function(what,limb)
if limb == owner.Character["Left Arm"] and (what:FindFirstAncestorWhichIsA("Model") and what:FindFirstAncestorWhichIsA("Model"):FindFirstChildWhichIsA("Humanoid") and what:FindFirstAncestorWhichIsA("Model"):FindFirstChildWhichIsA("Humanoid").Health ~= 0) and candamage then
candamage = false
punch.Parent = owner.Character.Head
punch.SoundId = "rbxassetid://"..punchtab[math.random(1,4)]
punch.Parent = nil
local he = what:FindFirstAncestorWhichIsA("Model"):FindFirstChildWhichIsA("Humanoid").Health
what:FindFirstAncestorWhichIsA("Model"):FindFirstChildWhichIsA("Humanoid").Health -= math.random(10,15)
coroutine.wrap(function()
for h = 1, 20 do
if (owner.Character.Head.Position - what.Position).Magnitude <= 14 then
wait(0.085)
owner.Character.Humanoid.Health += (he - what:FindFirstAncestorWhichIsA("Model"):FindFirstChildWhichIsA("Humanoid").Health)/10
end
end
end)()
splat(what,math.random(2,3))
end
end)
coroutine.wrap(function()
while true do
task.wait()
if owner.Character.Humanoid.MoveDirection ~= Vector3.new(0,0,0) and owner.Character.Humanoid.FloorMaterial ~= Enum.Material.Air then
moving = true
local c = owner.Character.HumanoidRootPart.CFrame:VectorToObjectSpace(owner.Character.Humanoid.MoveDirection)
wel.C0 = wel.C0:Lerp(CFrame.Angles(math.rad(-c.Z) * -5, math.rad(-c.X) * 20, math.rad(-c.X) * 10), 0.1)
else
wel.C0 = wel.C0:Lerp(CFrame.Angles(0,0,0), 0.1)
if moving then
moving = false
tween:Cancel()
tween2:Cancel()
goal2.C0 = savedBaseCFrame2
info2 = TweenInfo.new(0.25, Enum.EasingStyle.Circular, Enum.EasingDirection.InOut, 0, false, 0)
tween2 = tweenservice:Create(weld2, info2, goal2) 
goal.C0 = savedBaseCFrame
info = TweenInfo.new(0.25, Enum.EasingStyle.Circular, Enum.EasingDirection.InOut, 0, false, 0)
tween = tweenservice:Create(weld, info, goal) 
tween3:Cancel()
tween4:Cancel()
goal3.C0 = savedBaseCFrame3
info3 = TweenInfo.new(0.25, Enum.EasingStyle.Circular, Enum.EasingDirection.InOut, 0, false, 0)
tween3 = tweenservice:Create(weld3, info3, goal3) 
goal4.C0 = savedBaseCFrame4
info4 = TweenInfo.new(0.25, Enum.EasingStyle.Circular, Enum.EasingDirection.InOut, 0, false, 0)
tween4 = tweenservice:Create(weld4, info4, goal4) 
task.wait()
tween:Play()
tween2:Play()
tween3:Play()
tween4:Play()
end
end
end
end)()
owner.Character.ChildAdded:Connect(function(what)
if what:IsA("Tool") then
toolwel.C0 = weld3.C0
toolequip:Play()
toolwel.Enabled = true
weld3.Enabled = false
end
end)
owner.Character.ChildRemoved:Connect(function(what)
if what:IsA("Tool") then
weld3.C0 = toolwel.C0
toolwel.Enabled = false
weld3.Enabled = true
end
end)
local grounded = true
coroutine.wrap(function()
while true do
task.wait()
if owner.Character.Humanoid.FloorMaterial == Enum.Material.Air and not owner.Character.Humanoid.Sit then
grounded = false
groundeded = false
fall:Play()
fall2:Play()
fall3:Play()
fall4:Play()
fall4.Completed:Wait()
fall5:Play()
fall6:Play()
fall7:Play()
fall8:Play()
fall8.Completed:Wait()
end
end
end)()
local land = Instance.new("Sound",owner.Character.Head)
land.Volume = 0.25
land.SoundId = "rbxassetid://6870001600"
coroutine.wrap(function()
while task.wait() do
if owner.Character.Humanoid.FloorMaterial ~= Enum.Material.Air then
fall5:Cancel()
fall6:Cancel()
fall7:Cancel()
fall8:Cancel()
fall4:Cancel()
fall3:Cancel()
fall2:Cancel()
fall:Cancel()
if not grounded then
land:Play()
end
if not grounded then
grounded = true
return1:Play()
return2:Play()
return3:Play()
return4:Play()
return4.Completed:Wait()
groundeded = true
end
end
end
end)()
coroutine.wrap(function()
while true do
task.wait()
if not moving and active and groundeded then
if not owner.Character.Humanoid.Sit then
sit:Cancel()
sit2:Cancel()
sit3:Cancel()
sit4:Cancel()
up:Play()
up2:Play()
end
if not owner.Character.Humanoid.Sit then
seated = false
idle:Play()
idle2:Play()
else
if not seated then
seated = true
sitt:Play()
sitt2:Play()
else
sit:Play()
sit2:Play()
end
end
idle3:Play()
idle4:Play()
idle5:Play()
idle6:Play()
idle6.Completed:Wait()
if not moving and groundeded then
if not owner.Character.Humanoid.Sit then
sit:Cancel()
sit2:Cancel()
sit3:Cancel()
sit4:Cancel()
up:Play()
up2:Play()
end
if not owner.Character.Humanoid.Sit then
idle7:Play()
idle8:Play()
else
sit3:Play()
sit4:Play()
end
idle9:Play()
idle10:Play()
idle11:Play()
idle12:Play()
idle12.Completed:Wait()
end
end
end
end)()
count = 0
coroutine.wrap(function()
while true do
if moving then
