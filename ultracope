    tb.TextStrokeTransparency = 0
tb.TextEditable = false
tb.TextColor3 = Color3.new(255/255, 20/255, 20/255)
    tb.TextSize = 25
    tb.Font = "Arcade"
tb.Size = UDim2.new(0.125,0,0.125,0)
tb.AnchorPoint = Vector2.new(0.5, 0.55)
tb.Position = UDim2.new(0.5, 0, 0.55, 0)
tb.Text = "HELL IS FULL."
tb.Parent = gui
hud.Pitch = 0.5
hud.Parent = owner.PlayerGui
hud.Parent = nil
wait(2)
hud.Pitch = 1
for i,v in pairs(gui:GetDescendants()) do
if not v:IsA("TextBox") then
local tweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(1,Enum.EasingStyle.Quint,Enum.EasingDirection.In)
local goal = {}
goal.Transparency = 1
local tween = tweenService:Create(v, tweenInfo, goal)
tween:Play()
else
coroutine.wrap(function()
wait(1)
local tweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(2,Enum.EasingStyle.Quint,Enum.EasingDirection.In)
local goal = {}
goal.TextTransparency = 1
goal.TextStrokeTransparency = 1
local tween = tweenService:Create(v, tweenInfo, goal)
tween:Play()
end)()
end
end
game:GetService("Debris"):AddItem(gui,7)
wait(1)
power:Play()
power.Completed:Wait()
owner.Character.Humanoid.WalkSpeed = 24
owner.Character.Humanoid.JumpPower = 65
active = true
for i,v in pairs(owner.Character:GetDescendants()) do
pcall(function()
if not v:IsA("Tool") then
v.TextureId = "rbxassetid://8550010558"
end
end)
end
local jump = Instance.new("Sound",owner.Character.Head)
jump.Volume = 1
jump.SoundId = "rbxassetid://6870001835"
local jumpdb = false
owner.Character.Humanoid.Jumping:Connect(function(yes)
if not jumpdb and yes then
jump:Play()
jumpdb = true
wait(0.1)
jumpdb = false
end
end)
local recentdamage = 16
local music = Instance.new("Sound",owner.Character.Head)
music.SoundId = "rbxassetid://8581546179"
music.Volume = 0
music.Looped = true
local curhealth = owner.Character.Humanoid.Health
owner.Character.Humanoid.HealthChanged:Connect(function(health)
if recentdamage == 16 and curhealth > health then
owner.Character.Humanoid.WalkSpeed = 35
curhealth = health
coroutine.wrap(function()
for i = 1,10 do
music.Volume += 0.05
wait(0.05)
end
end)()
music:Play()
coroutine.wrap(function()
repeat
recentdamage  -= 1
wait(1)
until recentdamage  == 0
coroutine.wrap(function()
for i = 1,10 do
music.Volume -= 0.05
wait(0.05)
end
music:Stop()
end)()
recentdamage  = 16
owner.Character.Humanoid.WalkSpeed = 24
end)()
elseif curhealth > health then
curhealth = health
recentdamage = 15
elseif curhealth < health then
curhealth = health
end
end)
owner.Character.Humanoid.Died:Connect(function()
local gui = Instance.new("ScreenGui",owner.PlayerGui)
game:GetService("Debris"):AddItem(gui,game:GetService("Players").RespawnTime)
    local tb = Instance.new("TextBox")
    tb.BackgroundTransparency = 1
    tb.TextStrokeTransparency = 0
tb.TextEditable = false
tb.TextColor3 = Color3.new(230/255, 215/255, 5/255)
    tb.TextSize = 25
    tb.Font = "Arcade"
tb.Size = UDim2.new(0.125,0,0.125,0)
tb.AnchorPoint = Vector2.new(0.5, 0.45)
tb.Position = UDim2.new(0.5, 0, 0.45, 0)
tb.Text = "WARNING!"
tb.Parent = gui
    local tb = Instance.new("TextBox")
    tb.BackgroundTransparency = 1
    tb.TextStrokeTransparency = 0
tb.TextEditable = false
tb.TextColor3 = Color3.new(255/255, 20/255, 20/255)
    tb.TextSize = 25
    tb.Font = "Arcade"
tb.Size = UDim2.new(0.125,0,0.125,0)
tb.AnchorPoint = Vector2.new(0.5, 0.5)
tb.Position = UDim2.new(0.5, 0, 0.5, 0)
tb.Text = "CRITICAL DAMAGE COMMENCING SHUTDOWN"
tb.Parent = gui
hud.Volume = 3
hud.SoundId = "rbxassetid://3848738002"
hud.PlayOnRemove = true
local opening = Instance.new("Frame",gui)
opening.AnchorPoint = Vector2.new(0.5, 2)
opening.Position = UDim2.new(0.5, 0, 2, 0)
opening.BackgroundColor3 = Color3.fromRGB(5,5,5)
opening.Size = UDim2.new(1,0,0.55,0)
local opening2 = Instance.new("Frame",gui)
opening2.AnchorPoint = Vector2.new(0.5, -1)
opening2.Position = UDim2.new(0.5, 0, -1, 0)
opening2.BackgroundColor3 = Color3.fromRGB(5,5,5)
opening2.Size = UDim2.new(1,0,0.55,0)
for i = 1, 6 do
wait(0.4)
hud.Pitch = 0.5 + (i * 0.0325) 
hud.Parent = owner.PlayerGui
hud.Parent = nil
end
local tweenService = game:GetService("TweenService")
local timeToFade = 0.5
local object = bloodpartfloor
local tweenInfo = TweenInfo.new(timeToFade)
local goal = {}
goal.AnchorPoint = Vector2.new(0.5, 0)
goal.Position = UDim2.new(0.5, 0, 0, 0)
local grow = tweenService:Create(opening2, tweenInfo, goal)
grow:Play()
local goal2 = {}
goal2.AnchorPoint = Vector2.new(0.5, 1)
goal2.Position = UDim2.new(0.5, 0, 1, 0)
local grow = tweenService:Create(opening, tweenInfo, goal2)
grow:Play()
end)
local timer = 6
local lasttext = nil
local tb = nil
owner.Chatted:Connect(function(msg)
if timer == 6 then
   local billboard = Instance.new("BillboardGui", owner.Character.Head)
    billboard.Adornee = owner.Character.Head
    billboard.Size = UDim2.new(6,0,1.5,0)
billboard.ExtentsOffset = Vector3.new(0,2.5,0)
coroutine.wrap(function()
hud.Pitch = 1
hud.Parent = owner.Character.Head
hud.Parent = nil
wait(0.1)
hud.Pitch = 0.9
hud.Parent = owner.Character.Head
hud.Parent = nil
end)()
    local frame = Instance.new("ImageLabel",billboard)
      frame.BackgroundTransparency = 1
        frame.Image = "http://www.roblox.com/asset/?id=9862119590"
        frame.Size = UDim2.new(1,0,1,0)
tb = Instance.new("TextBox")
    tb.BackgroundTransparency = 0.999
    tb.TextStrokeTransparency = 0
tb.TextEditable = false
tb.TextColor3 = Color3.new(255/255, 20/255, 20/255)
    tb.Font = "Arcade"
tb.Size = UDim2.new(1,0,1,0)
tb.AnchorPoint = Vector2.new(0.5, 0.5)
tb.Position = UDim2.new(0.5, 0, 0.5, 0)
tb.Text = msg
tb.TextWrapped = true
tb.TextScaled = true
tb.MultiLine = true
tb.TextXAlignment = "Center"
tb.TextYAlignment = "Center"
tb.Parent = billboard
lasttext = msg
coroutine.wrap(function()
repeat
timer -= 1
wait(1)
until timer == 0
billboard:Destroy()
timer = 6
end)()
else
timer = 5
tb.Text = lasttext.." "..msg
lasttext = lasttext.." "..msg
coroutine.wrap(function()
hud.Pitch = 1
hud.Parent = owner.Character.Head
hud.Parent = nil
wait(0.1)
hud.Pitch = 0.9
hud.Parent = owner.Character.Head
hud.Parent = nil
end)()
end
end)
